
partial alphanumeric_keys
xkb_symbols "us" {

    name[Group1]= "English (Workman)";

    include "us(basic)"

    key.type[group1]="TWO_LEVEL";

    key <AD01> {[ q, Q ]};
    key <AD02> {[ d, D ]};
    key <AD03> {[ r, R ]};
    key <AD04> {[ w, W ]};
    key <AD05> {[ b, B ]};
    key <AD06> {[ NoSymbol, NoSymbol ]};
    key <AD07> {[ f, F ]};
    key <AD08> {[ u, U ]};
    key <AD09> {[ p, P ]};
    key <AD10> {[ j, J ]};
    key <AD11> {[ less, J ]};
    key <AD12> {[ greater, J ]};

    key <AC01> {[ a, A ]};
    key <AC02> {[ s, S ]};
    key <AC03> {[ h, H ]};
    key <AC04> {[ t, T ]};
    key <AC05> {[ g, G ]};
    key <AC06> {[ y, Y ]};
    key <AC07> {[ n, N ]};
    key <AC08> {[ e, E ]};
    key <AC09> {[ o, O ]};
    key <AC10> {[ i, I ]};
    key <AC11> {[ Return, Return ]};

    key <AB01> {[ z, Z ]};
    key <AB02> {[ x, X ]};
    key <AB03> {[ c, C ]};
    key <AB04> {[ v, V ]};
    key <AB05> {[ slash, question ]};
    key <AB06> {[ k, K ]};
    key <AB07> {[ l, L ]};
    key <AB08> {[ m, M ]};
    key <AB09> {[ comma, period ]};
    key <AB10> {[ semicolon, colon ]};
    
};

partial alphanumeric_keys 
xkb_symbols "planck_40"  {

    include "level5"
    key.type[group1]="EIGHT_LEVEL";

    key <AD01> {[ q         , Q         , NoSymbol, NoSymbol, 0           , NoSymbol, NoSymbol, NoSymbol ]};
    key <AD02> {[ d         , D         , NoSymbol, NoSymbol, 1           , NoSymbol, NoSymbol, NoSymbol ]};
    key <AD03> {[ r         , R         , NoSymbol, NoSymbol, 2           , NoSymbol, NoSymbol, NoSymbol ]};
    key <AD04> {[ w         , W         , NoSymbol, NoSymbol, 3           , NoSymbol, NoSymbol, NoSymbol ]};
    key <AD05> {[ b         , B         , NoSymbol, NoSymbol, 4           , NoSymbol, NoSymbol, NoSymbol ]};
    key <AD06> {[ minus     , underscore, NoSymbol, NoSymbol, asciitilde  , NoSymbol, NoSymbol, NoSymbol ]};
    key <AD07> {[ f         , F         , NoSymbol, NoSymbol, bracketleft , NoSymbol, NoSymbol, NoSymbol ]};
    key <AD08> {[ u         , U         , NoSymbol, NoSymbol, bracketright, NoSymbol, NoSymbol, NoSymbol ]};
    key <AD09> {[ p         , P         , NoSymbol, NoSymbol, braceleft   , NoSymbol, NoSymbol, NoSymbol ]};
    key <AD10> {[ j         , J         , NoSymbol, NoSymbol, braceright  , NoSymbol, NoSymbol, NoSymbol ]};
    key <AD11> {[ apostrophe, quotedbl  , NoSymbol, NoSymbol, grave       , NoSymbol, NoSymbol, NoSymbol ]};
    key <AD12> {[ slash     , backslash , NoSymbol, NoSymbol, bar         , NoSymbol, NoSymbol, NoSymbol ]};
    key <AC01> {[ a         , A         , NoSymbol, NoSymbol, 5           , NoSymbol, NoSymbol, NoSymbol ]};
    key <AC02> {[ s         , S         , NoSymbol, NoSymbol, 6           , NoSymbol, NoSymbol, NoSymbol ]};
    key <AC03> {[ h         , H         , NoSymbol, NoSymbol, 7           , NoSymbol, NoSymbol, NoSymbol ]};
    key <AC04> {[ t         , T         , NoSymbol, NoSymbol, 8           , NoSymbol, NoSymbol, NoSymbol ]};
    key <AC05> {[ g         , G         , NoSymbol, NoSymbol, 9           , NoSymbol, NoSymbol, NoSymbol ]};
    key <AC06> {[ y         , Y         , NoSymbol, NoSymbol, plus        , NoSymbol, NoSymbol, NoSymbol ]};
    key <AC07> {[ n         , N         , NoSymbol, NoSymbol, parenleft   , NoSymbol, NoSymbol, NoSymbol ]};
    key <AC08> {[ e         , E         , NoSymbol, NoSymbol, parenright  , NoSymbol, NoSymbol, NoSymbol ]};
    key <AC09> {[ o         , O         , NoSymbol, NoSymbol, less        , NoSymbol, NoSymbol, NoSymbol ]};
    key <AC10> {[ i         , I         , NoSymbol, NoSymbol, greater     , NoSymbol, NoSymbol, NoSymbol ]};
    key <AC11> {[ semicolon , colon     , NoSymbol, NoSymbol, asciicircum , NoSymbol, NoSymbol, NoSymbol ]};
    key <AB01> {[ z         , Z         , NoSymbol, NoSymbol, at          , NoSymbol, NoSymbol, NoSymbol ]};
    key <AB02> {[ x         , X         , NoSymbol, NoSymbol, numbersign  , NoSymbol, NoSymbol, NoSymbol ]};
    key <AB03> {[ c         , C         , NoSymbol, NoSymbol, dollar      , NoSymbol, NoSymbol, NoSymbol ]};
    key <AB04> {[ v         , V         , NoSymbol, NoSymbol, percent     , NoSymbol, NoSymbol, NoSymbol ]};
    key <AB05> {[ slash     , backslash , NoSymbol, NoSymbol, ampersand   , NoSymbol, NoSymbol, NoSymbol ]};
    key <AB06> {[ k         , K         , NoSymbol, NoSymbol, asterisk    , NoSymbol, NoSymbol, NoSymbol ]};
    key <AB07> {[ l         , L         , NoSymbol, NoSymbol, NoSymbol    , NoSymbol, NoSymbol, NoSymbol ]};
    key <AB08> {[ m         , M         , NoSymbol, NoSymbol, NoSymbol    , NoSymbol, NoSymbol, NoSymbol ]};
    key <AB09> {[ comma     , period    , NoSymbol, NoSymbol, NoSymbol    , NoSymbol, NoSymbol, NoSymbol ]};
    key <AB10> {[ space     , Tab       , NoSymbol, NoSymbol, NoSymbol    , NoSymbol, NoSymbol, NoSymbol ]};

    key.type[Group1] = "ONE_LEVEL";
    key <SPCE> { [ISO_Level5_Shift], actions[Group1]= [ SetMods(modifiers=LevelFive)] };
};


